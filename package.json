{
  "name": "shtp-maps",
  "version": "2.0.0",
  "description": "Interactive map application for Saigon High-Tech Park (Khu Công nghệ cao Sài Gòn)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.html --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.html --fix",
    "format": "prettier --write \"**/*.{js,html,css,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,html,css,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "security:audit": "npm audit --audit-level=moderate",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {}\""
  },
  "dependencies": {
    "ol": "^8.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.15.0",
    "eslint-plugin-html": "^8.1.2",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "terser": "^5.44.1",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.{js,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "map",
    "gis",
    "vietnam",
    "saigon",
    "high-tech-park",
    "openlayers",
    "routing",
    "navigation"
  ],
  "author": "",
  "license": "MIT"
}
